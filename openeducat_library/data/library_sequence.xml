<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">

        <!-- Library Card Sequence -->
        <record id="seq_library_card" model="ir.sequence">
            <field name="name">Library Card Sequence</field>
            <field name="code">op.library.card</field>
            <field name="prefix">LC</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>

        <!-- Media Movement Sequence -->
        <record id="seq_media_movement" model="ir.sequence">
            <field name="name">Media Movement Sequence</field>
            <field name="code">op.media.movement</field>
            <field name="prefix">MM</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>

    </data>
</odoo>