<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Scheduled Action: Check Overdue Borrowings -->
    <record id="ir_cron_check_overdue_borrowings" model="ir.cron">
        <field name="name">Library: Check Overdue Borrowings</field>
        <field name="model_id" ref="model_library_borrowing"/>
        <field name="state">code</field>
        <field name="code">model._cron_check_overdue_borrowings()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="active" eval="True"/>
        <field name="doall" eval="False"/>
    </record>
</odoo>